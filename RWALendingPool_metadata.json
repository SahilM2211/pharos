				"calculateInterest(uint256,uint256)": {
					"details": "Calculates the interest accrued on a loan."
				},
				"depositCollateralAndBorrow(address,uint256,uint256)": {
					"details": "Allows a user to deposit their RWA NFT as collateral and borrow stablecoins. The user must first approve this contract to manage their NFT."
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby disabling any functionality that is only available to the owner."
				},
				"repayLoanAndWithdrawCollateral(address,uint256)": {
					"details": "Allows the original borrower to repay their loan plus interest to reclaim their collateral. The borrower must first approve this contract to spend their stablecoins."
				},
				"setAssetValue(address,uint256,uint256)": {
					"details": "Sets or updates the value of an RWA NFT. In a real system, this would be managed by a trusted oracle service.",
					"params": {
						"_nftContract": "The address of the RWA NFT collection.",
						"_tokenId": "The specific ID of the NFT.",
						"_value": "The appraised value in USD (as a whole number)."
					}
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				}
